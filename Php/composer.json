{
  "name": "slim/slim-skeleton",
  "description": "A Slim Framework skeleton application for rapid development",
  "keywords": [
    "microframework",
    "rest",
    "router",
    "psr7"
  ],
  "homepage": "http://github.com/slimphp/Slim-Skeleton",
  "license": "MIT",
  "authors": [],
  "require": {
    "php": "^7.2",
    "ext-SimpleXML": "*",
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-xml": "*",
    "ext-xmlreader": "*",
    "ext-xmlwriter": "*",
    "ext-zip": "*",
    "ext-zlib": "*",
    "firebase/php-jwt": "^5.2",
    "google/apiclient": "^2.7",
    "monolog/monolog": "^2.0",
    "php-di/php-di": "^6.1",
    "phpoffice/phpspreadsheet": "^1.14",
    "slim/psr7": "^1.1",
    "slim/slim": "^4.5",
    "slim/twig-view": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "classmap": [
      "vendor/google/apiclient/src/Google"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S localhost:3000 -t public",
    "test": "phpunit"
  }
}
